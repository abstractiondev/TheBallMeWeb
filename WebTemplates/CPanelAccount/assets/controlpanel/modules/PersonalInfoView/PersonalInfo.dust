<div class="row">
    <div class="small-12 medium-12 large-12 columns">
        <h4 class="boldieText">My Personal Settings</h4>
    </div>
    <div class="small-6 medium-3 large-3 columns discreetTextColor">
        <label class="boldieText darkTextColor">Profile Image</label>
        <img class="oipfile" data-oipfile-filegroupid="personalProfileImage" data-oipfile-noimageurl="../assets/controlpanel/images/lightGray.jpg" style="width: auto;height:auto;">
        <input type="file" name="tmpProfileImage" style="display: none">
    </div>
    <br>
    <div class="small-12 medium-6 large-6 columns">
        <label class="boldieText darkTextColor">First Name<input type="text" class="pageEditingInputField" name="FirstName" /></label>
    </div>
    <br>
    <div class="small-12 medium-6 large-6 columns">
        <label class="boldieText darkTextColor">Last Name<input type="text" class="pageEditingInputField" name="LastName" /></label>
    </div>
</div>
<div class="row" id="groupSettings-buttonsRow">
    <div class="small-8 medium-3 large-4 columns"><a class="button oip-controller-command" data-oip-command="Save" style="float: right;">Save Changes</a></div>
    <div class="small-12 medium-6 large-6 columns"></div>
</div>

